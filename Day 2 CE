  return (
    <div style={{color:"drakblue" , padding:"10px" , border:"1px solid blue"  , borderRadius:"5px" , backgroundColor:"lightblue"}}>
    <h1 style ={{color:"green"}}>Inline Style in JSX Example</h1>
    <p style ={{fontSize:"16px", color:"darkblue" }}>This is a paragraph with inline styles applied<br></br></p>
    </div>
  )
}
